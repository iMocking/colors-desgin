{
  "name": "im-design",
  "version": "0.0.1",
  "description": "A Vue 3 UI Component Library",
  "type": "module",
  "main": "./dist/im-design.umd.cjs",
  "module": "./dist/im-design.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/im-design.js",
      "require": "./dist/im-design.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "docs:dev": "vite -c vite.docs.config.ts",
    "docs:build": "vite build -c vite.docs.config.ts"
  },
  "keywords": [
    "vue3",
    "ui",
    "component",
    "library",
    "tailwindcss"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  },
  "dependencies": {
    "im-design": "^1.4.50"
  }
}
